{
  "name": "cloudfl",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build ",
    "dev": "remix dev --manual -c \"npm run start\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "typecheck": "tsc",
    "pages:deploy": "npm run build && wrangler pages deploy ./public",
    "wrangler:update": "npm install wrangler@latest",
    "generate": "drizzle-kit generate:pg",
    "killPort": "lsof -t -i:8788 | xargs kill -9",
    "clarance:seed": "npx wrangler d1 execute clarance --local --file=./database/tables/0000_petite_tattoo.sql"
  },
  "dependencies": {
    "@cloudflare/pages-plugin-cloudflare-access": "^1.0.3",
    "@reduxjs/toolkit": "^2.0.1",
    "@remix-run/cloudflare": "^2.4.0",
    "@remix-run/cloudflare-pages": "^2.4.0",
    "@remix-run/css-bundle": "^2.4.0",
    "@remix-run/express": "^2.3.1",
    "@remix-run/node": "^2.3.1",
    "@remix-run/react": "^2.4.0",
    "@tsndr/cloudflare-worker-jwt": "^2.5.1",
    "bcrypt-ts": "^5.0.2",
    "drizzle-orm": "^0.29.4",
    "isbot": "^3.6.8",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "plaid": "^9.2.0",
    "plaid-threads": "^11.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plaid-link": "^3.5.1",
    "react-redux": "^9.0.3",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-run/dev": "^2.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.1.0",
    "vitest": "^1.1.0",
    "wrangler": "^3.29.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
